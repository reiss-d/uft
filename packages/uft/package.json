{
   "name": "uft",
   "version": "0.2.1",
   "license": "MIT",
   "description": "A collection of performant, strongly typed utilities to aid your Typescript application.",
   "homepage": "https://github.com/reiss-d/uft#readme",
   "repository": "https://github.com/reiss-d/uft.git",
   "bugs": "https://github.com/reiss-d/uft/issues",
   "author": "Reiss Davidson",
   "publishConfig": {
      "access": "public",
      "directory": "dist",
      "linkDirectory": true
   },
   "exports": {
      "./package.json": "./package.json",
      ".": {
         "import": {
            "types": "./esm/index.d.mts",
            "default": "./esm/index.mjs"
         },
         "require": {
            "types": "./cjs/index.d.ts",
            "default": "./cjs/index.js"
         }
      },
      "./*": {
         "import": {
            "types": "./esm/*.d.mts",
            "default": "./esm/*.mjs"
         },
         "require": {
            "types": "./cjs/*.d.ts",
            "default": "./cjs/*.js"
         }
      }
   },
   "typesVersions": {
      "*": {
         "esm/*": [
            "esm/*.d.mts",
            "esm/*"
         ],
         "cjs/*": [
            "cjs/*.d.ts",
            "cjs/*"
         ],
         "*": [
            "esm/*",
            "cjs/*"
         ]
      }
   },
   "main": "./cjs/index.js",
   "module": "./esm/index.mjs",
   "types": "./cjs/index.d.ts",
   "files": [
      "**"
   ],
   "sideEffects": false,
   "scripts": {
      "build": "pnpm clean && concurrently \"node ./transpile.mjs\" \"pnpm emit\" \"pnpm copy\" && node ./post-build.mjs",
      "clean": "shx rm -rf dist && shx mkdir -p dist",
      "copy": " shx cp ../../README.md package.json LICENSE dist && json -I -f dist/package.json -e \"this.publishConfig=undefined; this.devDependencies=undefined; this.scripts=undefined;\"",
      "emit": "tsup-node --config tsup.config.ts --dts-only",
      "test": "jest",
      "typedoc": "typedoc",
      "release": ""
   },
   "dependencies": {
      "@swc/helpers": "^0.5.1"
   },
   "devDependencies": {
      "@swc/cli": "^0.1.62",
      "@swc/core": "^1.3.82",
      "@swc/helpers": "^0.5.1",
      "@swc/jest": "^0.2.29",
      "@types/jest": "^29.5.3",
      "expect-type": "^0.16.0",
      "jest": "^29.6.2",
      "jest-chain": "^1.1.6",
      "jest-extended": "^4.0.1",
      "tsconfig": "workspace:*",
      "tsup": "^7.2.0",
      "typescript": "^5.2.2"
   },
   "keywords": [
      "functional",
      "toolkit",
      "types",
      "typesafe",
      "typescript",
      "utilities",
      "utils"
   ]
}
